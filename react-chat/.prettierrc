{
  "printWidth": 80,
  "tabWidth": 4,
  "arrowParens": "avoid",
  "trailingComma": "all",
  "singleQuote": true,
  "semi": false,
  "importOrder": [
    "^react(.*)",
    "antd/(.*)",
    "<THIRD_PARTY_MODULES>",
    "@/(.*)",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}